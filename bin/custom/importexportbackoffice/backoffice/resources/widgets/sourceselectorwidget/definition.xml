                   id="com.aimprosoft.importexportbackoffice.widgets.sourceselectorwidget.sourceselectorwidget">
    <name>Source Selector</name>
    <description>Source selector widget</description>
    <defaultTitle>Source selector</defaultTitle>
    <author>aimprosoft</author>
    <version>0.1</version>

    <sockets>
        <input id="reset" type="com.aimprosoft.importexportcloud.facades.data.StorageConfigData"/>
        <input id="inputStorageConfigData" type="com.aimprosoft.importexportcloud.facades.data.StorageConfigData"/>
        <input id="selectedScope" type="java.lang.String"/>
