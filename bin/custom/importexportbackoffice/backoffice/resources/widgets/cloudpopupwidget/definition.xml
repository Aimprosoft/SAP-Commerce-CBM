<?xml version="1.0" encoding="UTF-8"?>
<widget-definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/widget-definition.xsd"
                   id="com.aimprosoft.importexportbackoffice.widgets.cloudpopupwidget">
    <name>Cloud popup</name>
    <description></description>
    <defaultTitle>Cloud popup</defaultTitle>
    <author>aimprosoft</author>
    <version>0.1</version>

    <sockets>
        <input id="inputImportConfig" type="com.aimprosoft.importexportcloud.facades.data.StorageConfigData"/>
        <input id="inputExportConfig" type="com.aimprosoft.importexportcloud.facades.data.StorageConfigData"/>
        <output id="chosenExportObject" type="java.util.Map"/>
        <output id="chosenImportObject" type="java.util.Map"/>
        <output id="close"/>
    </sockets>

    <controller class="com.aimprosoft.importexportbackoffice.widgets.cloudpopupwidget.CloudFileChooserController"/>

    <keywords>
        <keyword>Import Export Module</keyword>
    </keywords>
</widget-definition>
