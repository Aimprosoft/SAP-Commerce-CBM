<web-fragment xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd" version="3.0">
    <display-name>mydropbox</display-name>
    <name>dropbox_fragment</name>

    <servlet>
            <servlet-name>dropboxServlet</servlet-name>
        <servlet-class>com.aimprosoft.DropboxResponseHandlerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dropboxServlet</servlet-name>
        <url-pattern>/parseDropboxAuthCode</url-pattern>
    </servlet-mapping>

</web-fragment>
